{
  "name": "@badeball/cypress-cucumber-preprocessor",
  "version": "2.2.0",
  "keywords": [
    "cypress",
    "cypress-plugin",
    "cypress-preprocessor"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/**/*.js",
    "lib/**/*.d.ts",
    "methods.js",
    "methods.d.ts",
    "specs-by-tags.js",
    "specs-by-tags.d.ts"
  ],
  "bin": {
    "specs-by-tags": "specs-by-tags.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc --watch",
    "test:types": "dtslint --expectOnly types",
    "test:unit": "mocha",
    "test:integration": "cucumber-js",
    "prepublishOnly": "npm run build && npm run test:types && npm run test:unit && npm run test:integration"
  },
  "dependencies": {
    "@cucumber/cucumber": "^7.0.0-rc.0",
    "@cucumber/cucumber-expressions": "^10.3.0",
    "@cucumber/gherkin": "^15.0.2",
    "@cucumber/messages": "^13.1.0",
    "@cucumber/tag-expressions": "^2.0.4",
    "@cypress/browserify-preprocessor": "^2.2.4",
    "cosmiconfig": "^4.0.0",
    "debug": "^4.2.0",
    "glob": "^7.1.6",
    "is-path-inside": "^3.0.2",
    "minimatch": "^3.0.4"
  },
  "devDependencies": {
    "@types/debug": "^4.1.5",
    "@types/glob": "^7.1.3",
    "cypress": "^5.5.0",
    "dtslint": "^4.0.5",
    "mocha": "^8.2.0",
    "stream-buffers": "^3.0.2",
    "strip-indent": "^3.0.0",
    "typescript": "^4.0.3"
  },
  "peerDependencies": {
    "cypress": "^4.0.0 || ^5.0.0"
  }
}
